{
  "presets": ["next/babel"],
  "plugins": [
    "transform-decorators-legacy", [
      "import", {
        "libraryName": "antd-mobile"
      }
    ],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": true,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}
